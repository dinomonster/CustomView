<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="CustomClockView">
        <attr name="radius" format="integer"/>
        <attr name="textSize" format="integer"/>
        <attr name="secondLen" format="integer"/>
        <attr name="minLen" format="integer"/>
        <attr name="hourLen" format="integer"/>
    </declare-styleable>

    <declare-styleable name="WaveView">
        <attr name="waveLen" format="integer"/>
        <attr name="waveCount" format="integer"/>
        <attr name="waveHeight" format="integer"/>
        <attr name="waveColor" format="integer"/>
    </declare-styleable>
</resources>